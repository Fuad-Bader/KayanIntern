
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - KayanIntern</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/KayanIntern.styles.css" asp-append-version="true" />
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700,900|Roboto+Slab:400,700" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <script src="https://kit.fontawesome.com/c4bce5b816.js" crossorigin="anonymous"></script>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons+Round" rel="stylesheet">
    <script src="~/js/material-dashboard.js"></script>
    <script src="~/assets/js/plugins/flatpickr.min.js"></script>
</head>

<body class="bg-gray-200">
    <div class="container position-sticky z-index-sticky top-0">
        <div class="row">
            <div class="col-12">
                <nav class="navbar navbar-expand-lg blur border-radius-xl top-0 z-index-3 shadow position-absolute my-3 py-2 start-0 end-0 mx-4">
                    <div class="container-fluid ps-2 pe-0">
                        <img src="~/img/KI logo 75x75.png" class="mx-2 img-fluid" alt="" style="width: 25px; height: auto;" />
                        <a class="navbar-brand ms-lg-0 ms-3 text-light" asp-controller="Home" asp-action="Index">
                            KayanIntern
                        </a>
                        <button class="navbar-toggler shadow-none ms-2" type="button" data-bs-toggle="collapse" data-bs-target="#navigation" aria-controls="navigation" aria-expanded="false" aria-label="Toggle navigation">
                            <span class="navbar-toggler-icon mt-2">
                                <span class="navbar-toggler-bar bar1"></span>
                                <span class="navbar-toggler-bar bar2"></span>
                                <span class="navbar-toggler-bar bar3"></span>
                            </span>
                        </button>
                        <div class="collapse navbar-collapse" id="navigation">
                            <ul class="navbar-nav mx-auto">
                                <li class="nav-item">
                                    <a class="nav-link d-flex align-items-center me-2 text-light" aria-current="page" href="#">
                                        <i class="fa fa-person-chalkboard opacity-6 text-light me-1"></i>
                                        Training Programs
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link me-2 text-light" asp-action="Index" asp-controller="Dashboard">
                                        <i class="fa fa-chart-pie opacity-6 text-light me-1"></i>
                                        Dashboard
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link me-2 text-light" href="#">
                                        <i class="fas fa-users opacity-6 text-light me-1"></i>
                                        Internships
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link me-2 text-light" href="#">
                                        <i class="fa-regular fa-circle-question opacity-6 text-light me-1"></i>
                                        FAQ
                                    </a>
                                </li>
                            </ul>
                            @if (ViewData["Title"] != "Login")
                            {
                                <ul class="navbar-nav d-lg-flex d-none">
                                    <li class="nav-item d-flex align-items-center">
                                        <a class="btn btn-primary btn-sm mb-0 me-2" target="_blank" asp-controller="Login" asp-action="Index">Sign In</a>
                                    </li>
                                </ul>
                            }
                        </div>
                    </div>
                </nav>
            </div>
        </div>
    </div>

    <main class="main-content  mt-0">
        <div class="page-header align-items-start min-vh-100" style="background-image: url('https://img2.wallspic.com/crops/6/8/2/5/7/175286/175286-ecoregion-natural_environment-mountain-biome-slope-1920x1080.jpg');">
            <span class="mask bg-gradient-dark opacity-6"></span>
            <div class="container skip-navbar">
                <main role="main" class="pb-3 mb-4">
                    @RenderBody()
                </main>
            </div>
        </div>
    </main>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.min.js" integrity="sha384-BBtl+eGJRgqQAUMxJ7pMwbEyER4l1g+O15P+16Ep7Q9Q+zqX6gSbd85u4mG4QzX+" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
    <script src="~/js/material-dashboard.js"></script>
    <script src="~/js/material-dashboard.min.js"></script>
    <script src="~/js/core/popper.min.js"></script>
    <script src="~/js/core/bootstrap.min.js"></script>
    <script src="~/js/plugins/perfect-scrollbar.min.js"></script>
    <script src="~/js/plugins/smooth-scrollbar.min.js"></script>
    <script async defer src="https://buttons.github.io/buttons.js"></script>
    <script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
    <script src="~/js/core/popper.min.js" type="text/javascript"></script>
    <script src="~/js/core/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/js/plugins/perfect-scrollbar.min.js"></script>
    <script src="~/js/plugins/countup.min.js"></script>
    <script src="~/js/site.js" type="text/javascript"></script>
</body>
</html>